<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vide.unifychatapplication">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_unifylogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_unifylogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NewUserRegisteration"></activity>
        <activity android:name=".PhoneAuthentication" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FetchPhoneContacts"/>
        <activity android:name=".MainActivity"></activity>
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />


    </application>

</manifest>